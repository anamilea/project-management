{
    "name": "roles/filter",
    "description": "A Laravel Nova filter.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Roles\\Filter\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Roles\\Filter\\FilterServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
